{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@11.6.2",
  "engines": {
    "node": ">=24.11.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24.11.1",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=11.6.2",
      "onFail": "error"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc -b",
    "@:eslint-inspect": "eslint --inspect-config",
    "@:node-modules-inspector": "npx node-modules-inspector",
    "@:nolyfill": "npx nolyfill"
  },
  "dependencies": {
    "@vueuse/core": "~14.1.0",
    "dompurify": "~3.3.0",
    "type-fest": "~5.3.0",
    "vue": "~3.5.25",
    "vue-component-type-helpers": "~3.1.5",
    "vue-router": "~4.6.3"
  },
  "devDependencies": {
    "@my/configs.js": "git+https://github.com/HotCherryPie/configs.js.git#semver:v0.0.0-alpha-12",
    "@my/utils.css": "git+https://github.com/HotCherryPie/utils.css.git#semver:v0.0.0-alpha-5",
    "@types/node": "~24.10.3",
    "@vitejs/plugin-vue": "~6.0.2",
    "eslint": "~9.39.1",
    "prettier": "~3.7.3",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.10",
    "vue-tsc": "~3.1.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.10",
    "is-core-module": "npm:@nolyfill/is-core-module@^1"
  }
}
